<?xml version="1.0" encoding="utf-8"?>
<test-set xmlns="http://www.w3.org/2010/09/qt-fots-catalog" name="fn-message" covers-40="fn-message">
   <description>Tests for the fn:message() function (4.0 proposal)</description>
 
   
   
   <dependency type="spec" value="XP40+ XQ40+"/>
   
   <test-case name="message-001">
      <description> Call on fn:message#1 </description>
      <created by="Michael Kay" on="2023-07-25"/>
      <test>message("Hi there!")</test>
      <result>
         <assert-empty/>
      </result>
   </test-case>
   
   <test-case name="message-002">
      <description> Call on fn:message#2 </description>
      <created by="Michael Kay" on="2023-07-25"/>
      <test>message("Hi there!", "Label")</test>
      <result>
         <assert-empty/>
      </result>
   </test-case>
   
   <test-case name="message-003">
      <description> Call on fn:message#2 </description>
      <created by="Michael Kay" on="2023-07-25"/>
      <test>message(("On", "the", "Ning", "Nang", "Nong"), "Milligan")</test>
      <result>
         <assert-empty/>
      </result>
   </test-case>


</test-set>