  </test-case>
  
  <test-case name="hash-003">
    <description>Unknown algorithm</description>
    <created by="Michael Kay" on="2024-03-28"/>
    <test>hash("xyz", {'algorithm':'romansch'})</test>
    <result>
      <error code="FOHA0001"/>
    </result>
  </test-case>

