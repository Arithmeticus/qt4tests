<test-set xmlns="http://www.w3.org/2010/09/qt-fots-catalog" name="fn-unique">
  <description>Tests for the fn:unique function</description>
  <link type="spec" document="http://www.w3.org/TR/xpath-functions-30/" idref="func-unique"/>
  <dependency type="spec" value="XP40+ XQ40+"/>
  <test-case name="unique-001">
    <description>Empty sequence</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(())]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-002">
    <description>Single integer</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(1)]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-003">
    <description>Single string</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique("x")]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-004">
    <description>Integers</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1, 1))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-005">
    <description>Identical integers</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1 to 1000) ! 1)]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-006">
    <description>Identical strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1 to 1000) ! 'x')]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-007">
    <description>Integer range</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1 to 2))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-008">
    <description>Integers</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1, 2, 3))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-009">
    <description>Strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1, 2, 3) ! string())]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-010">
    <description>Items of different type</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1, '1'))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-011">
    <description>Numbers of different type</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique((1, 1.0, 1e0))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-012">
    <description>Element with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:unique(<a/>[. = ''])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-013">
    <description>Elements with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:unique((<a/>, <b/>)[. = ''])]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-014">
    <description>Element with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:unique(<a/>[. != ''])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-015">
    <description>Elements with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:unique((<a/>, <b/>)[. != ''])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-016">
    <description>Empty array</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique([])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-017">
    <description>Array with single member</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique([ 1 ])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-018">
    <description>Array with uniform members</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique([ 1, 1 ])]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-019">
    <description>Array with distinct members</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique([ 1, 2 ])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-020">
    <description>Dynamic range expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:unique((1 to <_>1</_>/text()))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-021">
    <description>Dynamic range expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:unique((1 to <_>2</_>/text()))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-022">
    <description>Replicated filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(replicate((1 to 100)[. < 1], 100))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-023">
    <description>Replicated filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(replicate((1 to 100)[. < 2], 100))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-024">
    <description>Replicated filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(replicate((1 to 100)[. < 3], 100))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-025">
    <description>Reversed strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(reverse((1 to 10) ! string()))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-026">
    <description>Sorted strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(sort((1 to 10) ! string()))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-027">
    <description>Strings and collation</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(('A', 'a'), "http://www.w3.org/2005/xpath-functions/collation/html-ascii-case-insensitive")]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="unique-028">
    <description>Strings and collation</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:unique(('A', 'b'), "http://www.w3.org/2005/xpath-functions/collation/html-ascii-case-insensitive")]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
</test-set>