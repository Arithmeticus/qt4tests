<?xml version="1.0" encoding="utf-8"?>
<test-set xmlns="http://www.w3.org/2010/09/qt-fots-catalog" name="fn-void" covers-40="fn-void">
   <description>Tests for the fn:log() function (4.0 proposal)</description>
   <link type="spec" document="http://www.w3.org/TR/xquery-30/"
         idref="doc-xquery30-QuantifiedExpr"/>
   <link type="spec" document="XQuery" section-number="3.11"
         idref="id-quantified-expressions"/>
   
   
   <dependency type="spec" value="XP40+ XQ40+"/>
   
   <test-case name="void-001">
      <description> Call on fn:void </description>
      <created by="Michael Kay" on="2023-07-25"/>
      <test>void("Hi there!")</test>
      <result>
         <assert-empty/>
      </result>
   </test-case>
   
   <test-case name="void-002">
      <description> Call on fn:void raising an error</description>
      <created by="Michael Kay" on="2023-10-30"/>
      <test>void(23 div (year-from-date(current-date()) - year-from-dateTime(current-date())))</test>
      <result>
         <any-of>
            <assert-empty/>
            <error code="FOAR0001"/>
            <error code="XPTY0004"/>
         </any-of>
      </result>
   </test-case>
   
   <test-case name="void-003">
      <description> Use void#1 function item </description>
      <created by="Michael Kay" on="2024-02-20"/>
      <test>(1 to 10) => for-each(if (current-date() lt xs:date('1900-01-01')) 
                                  then fn:identity#1 
                                  else fn:void#1)</test>
      <result>
         <assert-empty/>
      </result>
   </test-case>
   
   <test-case name="void-004">
      <description> Use void#0 function item </description>
      <created by="Michael Kay" on="2024-02-20"/>
      <test>(1 to 10) => for-each(if (current-date() lt xs:date('1900-01-01')) 
                                  then fn:identity#1 
                                  else fn:void#0)</test>
      <result>
         <assert-empty/>
      </result>
   </test-case>
   
   <test-case name="void-005">
      <description> Direct call on void#0 </description>
      <created by="Michael Kay" on="2024-02-20"/>
      <test>empty(void())</test>
      <result>
         <assert-true/>
      </result>
   </test-case>
   


</test-set>