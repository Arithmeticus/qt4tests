<test-set xmlns="http://www.w3.org/2010/09/qt-fots-catalog" name="fn-uniform">
  <description>Tests for the fn:uniform function</description>
  <link type="spec" document="http://www.w3.org/TR/xpath-functions-30/" idref="func-uniform"/>
  <dependency type="spec" value="XP40+ XQ40+"/>
  <test-case name="uniform-001">
    <description>Empty sequence</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(())]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-002">
    <description>Single integer</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(1)]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-003">
    <description>Single string</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform("x")]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-004">
    <description>Integers</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1, 1))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-005">
    <description>Identical integers</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1 to 1000) ! 1)]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-006">
    <description>Identical strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1 to 1000) ! 'x')]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-007">
    <description>Integer range</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1 to 2))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-008">
    <description>Integers</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1, 2, 3))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-009">
    <description>Strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1, 2, 3) ! string())]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-010">
    <description>Items of different type</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1, '1'))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-011">
    <description>Numbers of different type</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform((1, 1.0, 1e0))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-012">
    <description>Element with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:uniform(<a/>[. = ''])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-013">
    <description>Elements with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:uniform((<a/>, <b/>)[. = ''])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-014">
    <description>Element with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:uniform(<a/>[. != ''])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-015">
    <description>Elements with filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:uniform((<a/>, <b/>)[. != ''])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-016">
    <description>Empty array</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform([])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-017">
    <description>Array with single member</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform([ 1 ])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-018">
    <description>Array with uniform members</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform([ 1, 1 ])]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-019">
    <description>Array with distinct members</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform([ 1, 2 ])]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-020">
    <description>Dynamic range expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:uniform((1 to <_>1</_>/text()))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-021">
    <description>Dynamic range expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <dependency type="spec" value="XQ40+"/>
    <test><![CDATA[fn:uniform((1 to <_>2</_>/text()))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-022">
    <description>Replicated filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(replicate((1 to 100)[. < 1], 100))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-023">
    <description>Replicated filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(replicate((1 to 100)[. < 2], 100))]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-024">
    <description>Replicated filter expression</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(replicate((1 to 100)[. < 3], 100))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-025">
    <description>Reversed strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(reverse((1 to 10) ! string()))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-026">
    <description>Sorted strings</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(sort((1 to 10) ! string()))]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-027">
    <description>Strings and collation</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(('A', 'a'), "http://www.w3.org/2005/xpath-functions/collation/html-ascii-case-insensitive")]]></test>
    <result>
      <assert-string-value>true</assert-string-value>
    </result>
  </test-case>
  <test-case name="uniform-028">
    <description>Strings and collation</description>
    <created by="Christian Gruen" on="2022-08-09"/>
    <test><![CDATA[fn:uniform(('A', 'b'), "http://www.w3.org/2005/xpath-functions/collation/html-ascii-case-insensitive")]]></test>
    <result>
      <assert-string-value>false</assert-string-value>
    </result>
  </test-case>
</test-set>