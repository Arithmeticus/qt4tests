<test-set xmlns="http://www.w3.org/2010/09/qt-fots-catalog" name="fn-range-to">
  <description>Tests for the fn:range-to function</description>
  <link type="spec" document="http://www.w3.org/TR/xpath-functions-30/" idref="func-range-to"/>
  <dependency type="feature" value="higherOrderFunctions"/>
  <dependency type="spec" value="XP40+ XQ40+"/>
  <test-case name="range-to-001">
    <description>Empty sequence, named function reference</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to((), boolean#1)]]></test>
    <result>
      <assert-empty/>
    </result>
  </test-case>
  <test-case name="range-to-002">
    <description>Single integer, named function reference</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to(0, boolean#1)]]></test>
    <result>
      <assert-eq>0</assert-eq>
    </result>
  </test-case>
  <test-case name="range-to-003">
    <description>Single integer, named function reference</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to(1, boolean#1)]]></test>
    <result>
      <assert-eq>1</assert-eq>
    </result>
  </test-case>
  <test-case name="range-to-004">
    <description>Integers, named function reference</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to((0, 1, 2, 3, 0), boolean#1)]]></test>
    <result>
      <assert-deep-eq>0, 1</assert-deep-eq>
    </result>
  </test-case>
  <test-case name="range-to-005">
    <description>Integer range, named function reference</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to(1 to 3, not#1)]]></test>
    <result>
      <assert-deep-eq>1, 2, 3</assert-deep-eq>
    </result>
  </test-case>
  <test-case name="range-to-006">
    <description>Integer range, function item</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to(1 to 3, function($n) { $n mod 2 = 0 })]]></test>
    <result>
      <assert-deep-eq>1, 2</assert-deep-eq>
    </result>
  </test-case>
  <test-case name="range-to-007">
    <description>Partial function application</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to(('January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'), contains(?, ''))]]></test>
    <result>
      <assert-string-value>January</assert-string-value>
    </result>
  </test-case>
  <test-case name="range-to-008">
    <description>Partial function application</description>
    <created by="Christian Gruen" on="2022-08-10"/>
    <test><![CDATA[fn:range-to(('January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'), starts-with(?, 'Feb'))]]></test>
    <result>
      <assert-deep-eq>'January', 'February'</assert-deep-eq>
    </result>
  </test-case>
</test-set>