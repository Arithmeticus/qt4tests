   <test-case name="fn-collation-028">
      <description>Recognized option supplied as untyped atomic</description>
      <created by="Michael Kay" on="2024-04-03"/>
      <test>collation({xs:untypedAtomic('lang'):'en'})</test>
      <result>
         <assert-string-value>http://www.w3.org/2013/collation/UCA?lang=en</assert-string-value>
      </result>
   </test-case>
   
   <test-case name="fn-collation-029">
      <description>Recognized option supplied as xs:anyURI</description>
      <created by="Michael Kay" on="2024-04-03"/>
      <test>collation({xs:anyURI('lang'):'en'})</test>
