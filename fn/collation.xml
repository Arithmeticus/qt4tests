      </result>
   </test-case>
   
   <test-case name="fn-collation-030">
      <description>Recognized option value is atomized</description>
      <created by="Michael Kay" on="2024-04-03"/>
      <test>collation({'lang':['en']})</test>
      <result>
         <assert-string-value>http://www.w3.org/2013/collation/UCA?lang=en</assert-string-value>
      </result>
   </test-case>
   
   <test-case name="fn-collation-031">
      <description>Extension boolean value</description>
      <created by="Michael Kay" on="2024-04-03"/>
      <test>collation({'private':false(), 'fallback':true()})</test>
      <result>
         <any-of>
            <assert-string-value>http://www.w3.org/2013/collation/UCA?private=no;fallback=yes</assert-string-value>
            <assert-string-value>http://www.w3.org/2013/collation/UCA?fallback=yes;private=no</assert-string-value>
         </any-of>
      </result>
   </test-case>

